      <titleStmt>
        <title>TEI P5 version 4.8.1 and Stylesheets version 7.57.1 release notes</title>
      </titleStmt>
      <editionStmt>
        <edition>
          <date when="2024-07-08">Monday, 8 July 2024</date>
        </edition>
      </editionStmt>
